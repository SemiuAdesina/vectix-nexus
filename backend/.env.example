# Server Configuration
PORT=3001
FRONTEND_URL=http://localhost:3000

# Fly.io Configuration
FLY_API_TOKEN=your_fly_api_token
FLY_API_HOSTNAME=https://api.machines.dev
FLY_APP_NAME=eliza-agent
FLY_IMAGE=registry.fly.io/eliza-agent:latest

# Solana Configuration
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
WALLET_MASTER_SECRET=your_wallet_master_secret_here

# Secrets Encryption
SECRETS_ENCRYPTION_KEY=your_secrets_encryption_key_here

# Stripe Configuration
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret
STRIPE_HOBBY_PRICE_ID=price_your_hobby_price_id
STRIPE_PRO_PRICE_ID=price_your_pro_price_id

# Clerk Configuration
CLERK_SECRET_KEY=sk_test_your_clerk_secret_key

# Database (PostgreSQL)
# Format: postgresql://USER:PASSWORD@HOST:PORT/DATABASE?sslmode=prefer
DATABASE_URL="postgresql://username:password@localhost:5432/vectix_nexus"

# CORS Configuration
TRUSTED_ORIGINS=http://localhost:3000

# Logging
LOG_LEVEL=info

# TEE (Trusted Execution Environment) Configuration
# Options: simulated (default), phala, intel-sgx, aws-nitro, azure, google-cloud
TEE_PROVIDER=simulated
TEE_ENCLAVE_ID=
TEE_ENCLAVE_KEY=

# Phala Network (Free tier available - https://cloud.phala.com)
PHALA_API_KEY=
PHALA_ENDPOINT=https://api.phala.network

# Intel Trust Authority (Free attestation)
INTEL_TRUST_AUTHORITY_URL=https://api.trustauthority.intel.com

# Azure Attestation (Free)
AZURE_ATTESTATION_URL=

# Google Cloud Attestation (Free)
GCP_PROJECT_ID=
GCP_REGION=

# OpenAI (Optional - for development/testing)
OPENAI_API_KEY=sk-your_openai_api_key

# Narrative Tracking (LunarCrush API)
# Get API key from: https://lunarcrush.com/developers
# Pricing: Individual plan starts at $72/month (10 req/min, 2,000/day)
# Free tier: Not available, but you can test with a trial if available
LUNARCRUSH_API_KEY=

# Enable demo mode to test UI without API key (uses mock data)
# Set to 'true' to enable demo mode in development
ENABLE_NARRATIVE_DEMO=true
